RewriteEngine On
RewriteBase /
RewriteRule ^/?$ index.php [L]
RewriteRule ^category/([0-9]*)/?$ /ali_category.php?id=$1 [L]
RewriteRule ^category/([0-9]*)-([^/]+)/?$ /ali_category.php?id=$1 [L]

RewriteRule ^category/([0-9]*)/([0-9]*)/?$ /ali_category.php?id=$1&page=$2 [L]
RewriteRule ^category/([0-9]*)-([^/]+)/([0-9]*)/?$ /ali_category.php?id=$1&page=$3 [L]

RewriteRule ^good/([0-9]*)/?$ /ali_good.php?id=$1 [L]
RewriteRule ^good/([0-9]*)-([^/]+)/?$ /ali_good.php?id=$1 [L]

RewriteRule ^redirect/([0-9]*)/?$ /ali_go.php?id=$1 [L]
RewriteRule ^search/?$ /ali_search.php [L]



